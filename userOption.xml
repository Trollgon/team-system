<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/userOption.xsd">
	<import>
		<options>
			<option name="uplayName">
				<categoryname>profile.contact</categoryname>
				<optiontype>text</optiontype>
				<validationpattern>^([a-zA-Z]{1}([a-zA-Z0-9_-]|.){2,14})?$</validationpattern>
				<showorder>7</showorder>
				<editable>3</editable>
				<visible>15</visible>
				<searchable>1</searchable>
			</option>
			<option name="uplayLegacyName">
				<categoryname>profile.contact</categoryname>
				<optiontype>text</optiontype>
				<validationpattern>^([a-zA-Z]{1}([a-zA-Z0-9_-]|.){2,14})?$</validationpattern>
				<showorder>7</showorder>
				<editable>3</editable>
				<visible>15</visible>
				<searchable>1</searchable>
			</option>
			<option name="psnID">
				<categoryname>profile.contact</categoryname>
				<optiontype>text</optiontype>
				<validationpattern>^([a-zA-Z]+[a-zA-Z0-9_-]{2,15})?$</validationpattern>
				<showorder>7</showorder>
				<editable>3</editable>
				<visible>15</visible>
				<searchable>1</searchable>
			</option>
			<option name="xboxLiveID">
				<categoryname>profile.contact</categoryname>
				<optiontype>text</optiontype>
				<validationpattern>^([a-zA-Z]{1}([a-zA-Z0-9]{2}|[a-zA-Z0-9] | [a-zA-Z0-9]){0,6}([a-zA-Z0-9]{1,3}|[a-zA-Z0-9] [a-zA-Z0-9]| [a-zA-Z0-9]([a-zA-Z0-9])?))?$</validationpattern>
				<showorder>7</showorder>
				<editable>3</editable>
				<visible>15</visible>
				<searchable>1</searchable>
			</option>
		</options>
	</import>
</data>